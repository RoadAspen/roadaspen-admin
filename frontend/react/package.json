  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "dev": "cross-env NODE_ENV=development npx webpack-dev-server --config webpack.config.js --colors",
    "mock": "cross-env NODE_ENV=development MOCK=true npx webpack-dev-server --config webpack.config.js --colors",
    "build": "cross-env NODE_ENV=production npx webpack --config webpack.config.js --colors",
    "watch": "npx webpack --config webpack.config.js --colors --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
